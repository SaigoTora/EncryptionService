@using EncryptionService.Core.Models.AsymmetricEncryption.KnapsackEncryption

@model KnapsackEncryptionResult

<span>N = @Model.N</span>
<span class="mb-3">M = @Model.M</span>

<span><b>Private key:</b></span>
<table class="mb-3 table-bordered">
	<tr>
		@for (int i = 0; i < Model.D.Length; i++)
		{
			<td class="p-2">
				@Model.D[i]
			</td>
		}
	</tr>
</table>

<span><b>Public key:</b></span>
<table class="mb-4 table-bordered">
	<tr>
		@for (int i = 0; i < Model.E.Length; i++)
		{
			<td class="p-2">
				@Model.E[i]
			</td>
		}
	</tr>
</table>

@await Html.PartialAsync("Encryption/_EncryptionResult", Model)